#
# import cz.dic as CZ
#
use locasploit.dictionary.import
set DICT CZ
set INPUTFILE /share/python/locasploit/dictionaries/cz.dic
run
#
# import cz_ascii.dic as CZ_ASCII
#
set DICT CZ_ASCII
set INPUTFILE /share/python/locasploit/dictionaries/cz_ascii.dic
run
#
# import en.dic as EN
#
set DICT EN
set INPUTFILE /share/python/locasploit/dictionaries/en.dic
run
q
